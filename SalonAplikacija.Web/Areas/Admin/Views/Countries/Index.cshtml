@model List<Country>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/_Layout.cshtml";
}



<div class="col-md-10">
    <!-- BASIC TABLE -->
    <div class="panel">
        <div class="panel-heading">
            <h3 class="panel-title">Basic Table</h3>
        </div>
        <div class="panel-body">
            <table class="table">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Name</th>
                        <th>Active</th>
                    </tr>
                </thead>
                <tbody>
                  @foreach(var country in Model)
                  {
                    <tr>
                        <td>@country.CountryId</td>
                        <td>@country.Name</td>
                        <td>@country.IsDeleted</td>
                    </tr>
                  }
                </tbody>
            </table>
        </div>
    </div>
    <!-- END BASIC TABLE -->
</div>


